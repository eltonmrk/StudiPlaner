<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res/com.studiplaner"
	android:orientation="vertical" android:background="#5b5c66">
	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_width="fill_parent" android:gravity="right|center_vertical"
		android:weightSum="1.0" android:background="#fbc166"
		android:layout_height="50dip">
		<LinearLayout android:id="@+id/LinearLayout03"
			android:layout_weight=".20" android:layout_width="0dip" android:layout_height="fill_parent">
			<TextView android:textStyle="bold"
				android:background="@layout/rounded_corner_number_empty"
				android:id="@+id/backToSemesterlistFromSB"
				android:gravity="center_vertical|center_horizontal"
				android:textColor="#9f7a3e" android:layout_height="37dip"
				android:layout_width="37dip" android:layout_gravity="center_vertical|center_horizontal"
				android:layout_marginLeft="5dip" android:text="&lt;" android:textSize="20dip"></TextView>
		</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayout04"
			android:layout_height="fill_parent" android:layout_weight=".60"
			android:layout_width="0dip" android:gravity="center_horizontal">
			<TextView android:id="@+id/semester_headername"
				android:text="no_name" android:gravity="center_vertical"
				android:layout_height="fill_parent" android:layout_width="wrap_content"
				android:textColor="#4f3b1b" android:textSize="18dip"></TextView>
		</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayout05"
			android:gravity="right" android:layout_weight=".20"
			android:layout_width="0dip" android:layout_height="fill_parent">
			<TextView android:textStyle="bold"
				android:background="@layout/rounded_corner_number_empty"
				android:text="+" android:id="@+id/add_veranstaltung"
				android:layout_marginRight="5dip" android:gravity="center_vertical|center_horizontal"
				android:textColor="#9f7a3e" android:layout_height="37dip"
				android:layout_width="37dip" android:layout_gravity="center_vertical|center_horizontal"
				android:textSize="25dip"></TextView>
		</LinearLayout>
	</LinearLayout>
	<ListView android:id="@+id/event_list" android:layout_height="wrap_content"
		android:layout_width="fill_parent"></ListView>
</LinearLayout>
