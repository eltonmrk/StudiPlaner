<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content"
	android:padding="10px" android:orientation="vertical">
	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:gravity="center_vertical" android:layout_marginBottom="5dip">
		<TextView android:layout_height="wrap_content" android:id="@+id/no_content"
			android:textSize="14dip" android:layout_width="wrap_content"
			android:text="@+id/no_content" android:textColor="#ffffff"></TextView>
	</LinearLayout>
	<LinearLayout android:layout_height="wrap_content"
		android:id="@+id/LinearLayout02" android:layout_width="fill_parent">
		<TextView android:layout_height="wrap_content" android:id="@+id/please_press"
			android:textSize="14dip" android:layout_width="wrap_content"
			android:text="@+id/no_content" android:textColor="#ffffff"></TextView>
		<TextView android:layout_height="30dip" android:textStyle="bold"
			android:id="@+id/no_addbutton" android:layout_marginLeft="5dip"
			android:textSize="14dip" android:layout_width="30dip"
			android:background="@layout/rounded_corner_square"
			android:text="+" android:gravity="center_vertical|center_horizontal"></TextView>
	</LinearLayout>
</LinearLayout>
