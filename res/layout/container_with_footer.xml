<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res/com.studiplaner"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:gravity="center_horizontal"
	android:background="@color/background">
	<com.markupartist.android.widget.ActionBar
		android:id="@+id/actionbar" style="@style/ActionBar" />
	<ListView android:layout_width="fill_parent"
		android:layout_below="@+id/actionbar" android:id="@+id/container_list"
		android:layout_height="wrap_content" android:divider="@color/seperator_color"
		android:dividerHeight="1px" android:layout_marginBottom="50dip"></ListView>
	<include layout="@layout/footer_select" />
</RelativeLayout>